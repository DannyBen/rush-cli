#!/usr/bin/env bash

# This bash completions script was generated by
# completely (https://github.com/dannyben/completely)
# Modifying it manually is not recommended
_rush_completions() {
  local cur=${COMP_WORDS[COMP_CWORD]}
  local comp_line="${COMP_WORDS[*]:1}"

  case "$comp_line" in
    'completions'*) COMPREPLY=($(compgen -W "--help -h" -- "$cur")) ;;
    'default'*) COMPREPLY=($(compgen -W "--help -h" -- "$cur")) ;;
    'remove'*) COMPREPLY=($(compgen -W "--help --purge -h -p" -- "$cur")) ;;
    'config'*) COMPREPLY=($(compgen -W "--edit --help -e -h" -- "$cur")) ;;
    'snatch'*) COMPREPLY=($(compgen -W "--help -h" -- "$cur")) ;;
    'search'*) COMPREPLY=($(compgen -W "--help -h" -- "$cur")) ;;
    'clone'*) COMPREPLY=($(compgen -A directory -W "--default --help --ignore --name --shallow --ssh -d -h -i -n -s -w" -- "$cur")) ;;
    'pull'*) COMPREPLY=($(compgen -W "--help -h" -- "$cur")) ;;
    'push'*) COMPREPLY=($(compgen -W "--all --help --message -a -h -m" -- "$cur")) ;;
    'undo'*) COMPREPLY=($(compgen -W "$(rush list -s -a) --help -h" -- "$cur")) ;;
    'copy'*) COMPREPLY=($(compgen -W "$(rush list -s -a) --force --help -f -h" -- "$cur")) ;;
    'info'*) COMPREPLY=($(compgen -W "$(rush list -s -a) --help -h" -- "$cur")) ;;
    'list'*) COMPREPLY=($(compgen -W "--all --help --simple -a -h -s" -- "$cur")) ;;
    'edit'*) COMPREPLY=($(compgen -W "$(rush list -s -a) --help -h" -- "$cur")) ;;
    'show'*) COMPREPLY=($(compgen -W "$(rush list -s -a) --help -h" -- "$cur")) ;;
    'add'*) COMPREPLY=($(compgen -A directory -W "--help -h" -- "$cur")) ;;
    'get'*) COMPREPLY=($(compgen -W "$(rush list -s -a) --clone --help -c -h" -- "$cur")) ;;
    ''*) COMPREPLY=($(compgen -W "$(rush list -s -a) --help --version -h -v add clone completions config copy default edit get info list pull push remove search show snatch undo" -- "$cur")) ;;
  esac
}

complete -F _rush_completions rush
