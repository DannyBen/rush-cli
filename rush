    # :command.usage_examples
    echo -e "Examples:"
    
    echo -e "  rush clone bobby"
    echo -e "  rush clone bobby/bobs-repo ./repos/bobby --ssh"
    echo -e "  rush clone bobby --name sample"
    echo

  fi
}

